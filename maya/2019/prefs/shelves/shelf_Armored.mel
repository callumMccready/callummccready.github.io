        -command "//\tArmored Wolf - Juan Hernández\n//\tRandomizeWireframeColor.mel 17/Jan/18\n\n//\tDescription:\n//\tAssigns random Wireframe colors to your selected objects.\n\n\n{\n// COLOR BIAS------------\n// I wouldnt go over 10. The more bias you add the less variance you will get. Best to only increase one or two.\n// Eg: red=10, green=0, blue=0.\n// Eg: red=7,  green=0, blue=7.\n// Eg: red=0,  green=0, blue=0.\n\nint $redB   = 0;\nint $greenB = 0;\nint $blueB  = 10;\n//------------------------\n\nint $minVal = 0;    // 0 Works best.\nint $maxVal = 9;    // This value is fine if you introduce any color bias close to 10.\n\n// Keeps the maxValue or Brightness of the wireframe from getting too high as you increase bias.\nfloat $bControl = ($redB + $greenB + $blueB) * 0.1 + 1;\n\nstring $selection[] = `ls -selection`;\n\nfor ($obj in $selection)\n{\n// Randomize and add Bias (also weird I know).\n\tfloat $ranRED   = rand( $minVal + $redB,   $maxVal + $redB/$bControl );\n\tfloat $ranGREEN = rand( $minVal + $greenB, $maxVal + $greenB/$bControl );\n\tfloat $ranBLUE  = rand( $minVal + $blueB,  $maxVal + $blueB/$bControl );\n\t\n// We use the 0.1 Multiplier to scale down our integers into smaller values\n\tcolor -rgb ($ranRED * 0.1) ($ranGREEN * 0.1) ($ranBLUE  * 0.1) $obj;\n}\n\nselect $selection;\n}\n" 
