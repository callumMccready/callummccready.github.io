        -mi "Single Instance to Object" ( "//  Instance cleanup script.\n\n  //Select the Groups instead of the objects inside.\n\npickWalk -d up;\nstring $selection[] = `ls -selection`;\nstring $tempString;\nstring $tempSel;\n\n\n\nif (size($selection) < 2)\n{\n\t$tempSel = `substring $selection[0] 1 (size($selection[0])-1)`;\n\n\tif ($tempSel == \"G_Original\")\n\t{\n\t\t$tempString = substituteAllString($selection[0], \"Original\", \"Instance\");\n\t}\n\n\telse\n\t{\n\t\t$tempString = substituteAllString($selection[0], \"Instance\", \"Original\");\n\t}\n\n\tselect -add $tempString;\n}\n\n$selection = `ls -selection`;\n\nselect -d \"G_Original*\";\nConvertInstanceToObject;\n\nselect $selection;\nungroup;\n\n\n\n\n\n" )
